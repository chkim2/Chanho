package com.d03.abs;

public class UnitTest {
	public static void main(String[] args) {
		Car c1 = new Car("승용차",50);
		c1.decEnergy();
		System.out.println("c1 의 energy : " + c1.getEnergy());
		
		Bus b1 = new Bus("마을버스",100);
		b1.decEnergy();
		System.out.println("b1 의 energy : " + b1.getEnergy());
		
		Truck t1 = new Truck("이사짐 센터 차",200);
		t1.decEnergy();
		System.out.println("t1 의 energy : " + t1.getEnergy());
	}

}
