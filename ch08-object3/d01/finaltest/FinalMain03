package com.d01.finaltest;

//클래스에 final을 며시하면 상속되지 않는 클래스가 됨
final class A2{
	int a =100;
	public void make() {
		System.out.println(a);
	}
}

//final 클래스는 상속 불가능
//class B extends A2{
//	
//}

public class FinalMain03 {
	public static void main(String[] args) {
		A2 a2 = new A2();
		a2.make();
	}
	
	

}
