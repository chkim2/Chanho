package com.d07.statictest;

public class StaticCount {
	//인스턴스 변수
	int c;
	//static(클래스) 변수
	static int count;
	
	//생성자
	public StaticCount() {
		StaticCount.count++;
		c++;
		
	}
	public static void main(String[] args) {
		StaticCount sc = new StaticCount();
		
		System.out.println(sc.count);
		System.out.println(sc.c);
		
	}
}
