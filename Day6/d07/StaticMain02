package com.d07.statictest;

public class StaticMain02 {
	//인스턴스 변수
	int a;
	//static 변수
	static String s;
	public static void main(String[] args) {
		//인스턴스 변수는 객체 생성 후 호출해야 함
		//System.out.println("a = " + a);
		
		//static 변수는 객체 생성과 무관하며 호출시
		//static 영에 만들어짐.
		//같은 클래스 내의 static 변수는 클래스명 없이 그냥 호출가능
		s= "자바의 꿈";
		System.out.println("s = "+s);

	}

}
