public class ArrayMain05 {
	public static void main(String[] args) {
		//배열 선언,생성, 초기화(암시적 배열생성)
		int[] score = {79,88,91,33,100,55,95};
		
		//기준값 설정
		int max = score[0];
		int min = score[0];
		for(int i =1; i<score.length; i++) {
			if(max<score[i])
				max = score[i];
			
			if(min>score[i])
				min = score[i];
		}
		System.out.println("max : "+max);
		System.out.println("min : "+min);
	}
}
