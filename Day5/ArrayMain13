public class ArrayMain13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] array = new int[9][8];
		
		for(int i=0; i<array.length;i++) {//1~9
			for(int j=0; j<array[i].length; j++) {//2~9
				array[i][j]=(j+2)*(i+1);
				System.out.print((j+2)+" * "+(i+1)+" = "+array[i][j]+"\t");
			}
			System.out.println("");
		}
	}

}
