public class ArrayMain20 {

	public static void main(String[] args) {
				 //String -> int 
		int num1 = Integer.parseInt(args[0]); 
		int num2 = Integer.parseInt(args[1]);
		
		System.out.println(args[0]+args[1]);
		System.out.println("======================");
		System.out.println(num1+num2);
	}

}
