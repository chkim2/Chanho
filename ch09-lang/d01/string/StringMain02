package com.d01.string;

public class StringMain02 {
	public static void main(String[] args) {
		String str1 = "bus";
		String str2 = "Bus";
		
		if(str1.equals(str2)) {
			System.out.println("같은 문자열");
		}else {
			System.out.println("다른 문자열");
		}
		
		System.out.println("==============================");
		//문자열 비교(대소문자 구별 없이 비교)
		if(str1.equalsIgnoreCase(str2)) {
			System.out.println("[대소문자 구별없이 비교했을 때 ] 같은 문자열");
		}else {
			System.out.println("[대소문자 구별없이 비교했을 때 ] 다른 문자열");
		}
	}

}
