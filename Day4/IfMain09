public class IFMain09 {
	public static void main(String[] args) {
		/*
		 * 제시된 문자가 문자, 소문자, 기타문자인지
		 * 체크하고 대문자,소문자, 기타문자로 출력하시
		 */
		char ch= 'Z';
		
		if(ch>='A'&& ch<='Z') {//대문자
			System.out.println(ch+"는 대문자 입니다.");
		}else if(ch>='a'&& ch<='z') {//소문자
			System.out.println(ch+"는 소문자 입니다.");
		}else 
			System.out.println(ch+"는 기타문자 입니다.");
			
	}

}
