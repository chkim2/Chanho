public class IfMain08 {
	public static void main(String[] args) {
		int a = 10;
		float b = 10.0f;
		
		if(a==b) {
			System.out.println("10과 10.0f는 같다.");
		}
		char c= '0';
		int d = 0;
		if(c!=d) {
			System.out.println("0과 0는 같지않다.");
		}
		char e = 'A';
		int f = 65;
		if(e==f){
			System.out.println("'A'는 65와 같다.");
		}
	}

}
