public class ContinueMain01 {
	public static void main(String[] args) {
		for(int i=0; i<=10; i++) {


			//특정 조건일 때 코드를 수행 하지 않고 다음 반복횟수차로 건너뜀
			if(i%3==0)//3의 배수
				continue;

			System.out.println(i);
		}
		System.out.println("---------------");
		//짝수만 출력
		for(int i=0; i<=10; i++) {


			//특정 조건일 때 코드를 수행 하지 않고 다음 반복횟수차로 건너뜀
			if(i%2!=0)//3의 배수
				continue;

			System.out.println(i);
		}
	}
}
