
public class WhileMain03 {

	public static void main(String[] args) {
		java.util.Scanner scan = new java.util.Scanner(System.in);
		System.out.print("단 입력 : ");
		int dan = scan.nextInt();
		
		int i = 1;//초기값
		while(i<=9) {
			System.out.println(dan + " * " + i + "=" + dan*i);
				i++;	
		}
		scan.close();
	}

}
