public class IfMain01 {

	public static void main(String[] args) {
		/*
		 * if(조건){
		 * 	수행문;
		 *  수행문;
		 * }
		 */
		int n = 10;
		//조건이true이면 if블럭안의 수행문을 실행
		if(n > 5) {
			System.out.printf("n은 5보다 크다.%n",n);
		}
		System.out.println("-------------------");
		if(n > 5) 
			System.out.printf("n은 5보다 크다.%n",n);
		
		System.out.println("프로그램 끝~~");

	}

}
