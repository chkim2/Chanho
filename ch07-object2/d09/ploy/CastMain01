package com.d09.poly;

import com.d09.poly.CastMain01;

class A{}
class B{}
class C{}
class D{}

public class CastMain01 {
	//
	//자료형과 관계없이  객체의 주소가 전달 될 수 있는 메소드
	//자식클래스 타입에서 부모 클래스 타입으로 형변환 하는 방식을 이용했음
	public void add(Object o) {}
	
	public static void main(String[] args) {
		CastMain01 c = new CastMain01();
		c.add(new A());//A->Object 업캐스팅
		c.add(new B());//B->Object 업캐스팅
		c.add(new C());//C->Object 업캐스팅
		c.add(new D());//D->Object 업캐스팅
	}

}
